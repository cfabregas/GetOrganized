<view class="mask {{!isShow && 'hide'}}">
  <view class="main">

    <view class="result-icon" style="background: {{iconColor}}">
      <text class="iconfont icon-{{icon}}"></text>
    </view>

    <view class="pick-zone">
      <scroll-view class="picker">123</scroll-view>
      <scroll-view class="picker">456</scroll-view>
    </view>

    <view class="close-btn" bind:tap="onClose">
      <icon type="cancel" size="40" color="#fff"></icon>
    </view>
  </view>
</view>
